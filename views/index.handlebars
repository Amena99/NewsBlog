<div class="container">
  <div class="row">

    <div class="col-12 my-3">
      <h2>This Week's News</h2>
{{!-- {{currentWeekDay}} --}}
    </div>

    <div class="col-sm-12 col-md-9">
      <div class="row">
          <p>this is the index file</p>
         <ul id="articles">  
          
            {{#each objects}}
            <li style="list-style-type: none">
             {{#if this.summary}}<p> Title: {{this.title}}</p>{{/if}}
             {{#if this.summary}}<p> Summary: {{this.summary}}</p>{{/if}}
             {{#if this.summary}}{{#if this.link}}<p> Link: <a href = "http://nytimes.com{{this.link}}" target='_blank'>Click here for the full article.</a></p>{{/if}}{{/if}}
            {{/each}}
            </li>
         </ul>
      </div>
    </div>
  </div>
</div>

<script src="/js/app.js"></script>